{
    "name": "n8n-nodes-gologin",
    "version": "0.1.0",
    "description": "n8n node for GoLogin API - manage browser profiles, proxies, workspaces and more",
    "license": "MIT",
    "homepage": "https://gologin.com",
    "keywords": [
        "n8n-community-node-package",
        "gologin",
        "browser",
        "automation",
        "proxy",
        "fingerprint"
    ],
    "author": {
        "name": "igortikushEasync",
        "email": "igor@easync.io"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gologinapp/n8n-nodes-gologin.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/GologinApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Gologin/Gologin.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "^2.0.0-rc.2"
    },
    "main": ".prettierrc.js",
    "bugs": {
        "url": "https://github.com/gologinapp/n8n-nodes-gologin/issues"
    }
}
